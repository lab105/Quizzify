{
  "routes": [
    {
      "src": "/api",
      "headers": {
        "Content-Security-Policy": "default-src 'self' https:; connect-src 'self' https://domain.example/"
      }
    },
    {
      "src": "/api/*",
      "headers": {
        "Content-Security-Policy": "default-src 'self' https:; connect-src 'self' https://domain.example/"
      }
    }
  ]
}
